{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleDirectories": ["node_modules", "<rootDir>/src", "src"],
  "rootDir": ".",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{ts,js}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ]
}
