{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleDirectories": ["node_modules", "<rootDir>/src", "src"],
  "rootDir": ".",
  "testRegex": ".load.ts$",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*{load,mock}.{ts,js}",
    "!**/node_modules/**",
    "!**/vendor/**"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
